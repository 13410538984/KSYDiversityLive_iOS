use_frameworks!

target 'KSYRKUploadExt' do
	pod 'GPUImage'
    pod 'libksygpulive/libksygpulive', '1.9.2'
end

target 'KSYReplayKitDemo' do
	pod 'GPUImage'
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    puts "!!!! #{target.name}"
  end
end
